{% extends 'base.html.twig' %}


        {% block title %}

            Company Name || Show?Users

        {% endblock title %}


        {% block websiteName %}
            <h3>
                Ilosc uzytkownikow: {{users|length}}
            </h3>
            <h4>
                <ul>

                    {% for user in users %}

                        <li>
                            {{ user.getLogin }}
                        </li>
                    {% else %}
                        Brak kategori
                    {% endfor %}
                </ul>
            </h4>
        {% endblock websiteName %}